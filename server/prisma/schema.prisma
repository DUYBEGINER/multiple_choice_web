generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model users {
  uid         String    @id(map: "PK__users__DD7012645CE56F39") @db.NVarChar(128)
  email       String    @unique(map: "UQ__users__AB6E61649A9D41E9") @db.NVarChar(256)
  displayName String?   @db.NVarChar(256)
  createdAt   DateTime? @default(now(), map: "DF__users__createdAt__5070F446")
  status      String?   @default("active", map: "DF__users__status__5165187F") @db.NVarChar(50)
}
